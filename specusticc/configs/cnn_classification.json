{
	"import": {
		"datatype": "real",
		"method": "random_walk",
		"gaussian_noise": false,
		"ticker": "PKO",
		"date": {
			"from": "2010-01-01",
			"to": "2018-12-31"
		}
	},
	"data": {
		"columns": [
			"close",
			"vol"
		],
		"sequence_length": 50,
		"test_size": 0.33,
		"normalise": true
	},
	"training": {
		"epochs": 100,
		"batch_size": 32
	},
	"model": {
		"save_dir": "saved_models",
		"target": "classification",
		"type": "neural_network",
		"layers": [
			{
				"type": "input",
				"input_timesteps": 50,
				"input_dim": 2
			},
			{
				"type": "conv1d",
				"neurons": 100,
				"kernel_size": 1
			},
			{
				"type": "averagePooling1d",
				"pool_size": 2
			},
			{
				"type": "conv1d",
				"neurons": 100,
				"kernel_size": 1
			},
			{
				"type": "averagePooling1d",
				"pool_size": 2
			},
			{
				"type": "dense",
				"neurons": 5,
				"activation": "linear"
			}
		],
		"loss": "categorical_crossentropy",
		"optimizer": "adam"
	}
}
