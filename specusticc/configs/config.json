{
	"data": {
		"ticker" : "CDR",
		"columns": [
			"close",
			"vol"
		],
		"date": {
			"from": "2010-01-01",
			"to": "2018-12-31"
		},
		"sequence_length": 50,
		"train_test_split": 0.66,
		"normalise": true
	},
	"training": {
		"epochs": 3,
		"batch_size": 32
	},
	"model": {
		"type": "neural_network",
		"loss": "mse",
		"optimizer": "adam",
		"save_dir": "saved_models",
		"layers": [
			{
				"type": "lstm",
				"neurons": 100,
				"input_timesteps": 49,
				"input_dim": 2,
				"return_seq": true
			},
			{
				"type": "dropout",
				"rate": 0.2
			},
			{
				"type": "lstm",
				"neurons": 100,
				"return_seq": true
			},
			{
				"type": "lstm",
				"neurons": 100,
				"return_seq": false
			},
			{
				"type": "dropout",
				"rate": 0.2
			},
			{
				"type": "dense",
				"neurons": 1,
				"activation": "linear"
			}
		]
	}
}
