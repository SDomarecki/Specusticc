{
	"import": {
		"input": [
			{
				"ticker": "PKO",
				"columns": [
					"open",
					"high",
					"low",
					"close",
					"vol"
				]
			}
		],
		"target": [
			{
				"ticker": "PKO",
				"columns": [
					"close"
				]
			}
		],
		"train_date": {
			"from": "2008-01-01",
			"to": "2015-12-31"
		},
		"test_date": {
			"from": "2016-01-01",
			"to": "2019-06-30"
		}
	},
	"preprocessing": {
		"sequence_length": 250,
		"sample_time_difference": 50,
		"sequence_prediction_time": 50
	},
	"model": {
		"target": "regression",
		"type": "neural_network",
		"layers": [
			{
				"type": "lstm",
				"neurons": 100,
				"input_timesteps": 49,
				"input_dim": 5,
				"return_seq": true
			},
			{
				"type": "dropout",
				"rate": 0.2
			},
			{
				"type": "lstm",
				"neurons": 100,
				"return_seq": true
			},
			{
				"type": "lstm",
				"neurons": 100,
				"return_seq": false
			},
			{
				"type": "dropout",
				"rate": 0.2
			},
			{
				"type": "dense",
				"neurons": 1,
				"activation": "linear"
			}
		],
		"compilation": {
			"loss": "sparse_categorical_crossentropy",
			"optimizer": "adam",
			"metrics": [
				"sparse_categorical_accuracy"
			]
		},
		"fitting":{
			"epochs": 10,
			"batch_size": 50
		}
	}
}
