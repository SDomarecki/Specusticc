{
	"import": {
		"datatype": "real",
		"ticker": "PKO",
		"date": {
			"from": "2010-01-01",
			"to": "2018-12-31"
		}
	},
	"data": {
		"columns": [
			"open",
			"high",
			"low",
			"close",
			"vol"
		],
		"features": [
			"roc",
			"macd_val",
			"rsi"
		],
		"sequence_length": 50,
		"test_size": 0.33,
		"normalise": true
	},
	"model": {
		"save_dir": "saved_models",
		"target": "classification",
		"type": "decision_tree",
		"max_depth": 10
	}
}
