{
	"import": {
		"datatype": "real",
		"method": "random_walk",
		"gaussian_noise": false,
		"ticker": "PKO",
		"date": {
			"from": "2008-01-01",
			"to": "2019-06-30"
		}
	},
	"data": {
		"columns": [
			"date",
			"open",
			"high",
			"low",
			"close",
			"vol"
		],
		"features": [
			"roc",
			"macd_val",
			"rsi"
		],
		"sequence_length": 250,
		"sequence_shift": 50,
		"prediction_shift": 50,
		"test_size": 0.33
	},
	"model": {
		"save_dir": "saved_models",
		"target": "classification",
		"type": "decision_tree",
		"max_depth": 10
	}
}
