{
	"import": {
		"input": {
			"tickers": [
				"PKO",
				"PZU",
				"CDR",
				"CCC",
				"11B"
			],
			"columns": [
				"open",
				"high",
				"low",
				"close",
				"vol"
			]
		},
		"target": {
			"tickers": [
				"PKO",
				"PZU",
				"CDR",
				"CCC",
				"11B"
			],
			"columns": [
				"close"
			]
		},
		"train_date": {
			"from": "2010-06-01",
			"to": "2015-12-31"
		},
		"test_date": {
			"from": "2016-01-01",
			"to": "2019-06-30"
		}
	},
	"preprocessing": {
		"sequence_length": 60,
		"sample_time_difference": 20,
		"sequence_prediction_time": 20
	},
	"model": {
		"target": "classification",
		"type": "neural_network",
		"layers": [
			{
				"type": "lstm",
				"neurons": 60,
				"input_timesteps": 60,
				"input_dim": 5,
				"return_seq": true
			},
			{
				"type": "lstm",
				"neurons": 20,
				"return_seq": false
			},
			{
				"type": "dropout",
				"rate": 0.4
			},
			{
				"type": "batchNormalization"
			},
			{
				"type": "dense",
				"neurons": 5,
				"activation": "softmax"
			}
		],
		"compilation": {
			"loss": "categorical_crossentropy",
			"optimizer": "adam",
			"metrics": [
				"categorical_accuracy"
			]
		},
		"fitting":{
			"epochs": 50,
			"batch_size": 100
		}
	}
}
