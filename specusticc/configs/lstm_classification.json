		"train_date": {
			"from": "2010-06-01",
			"to": "2015-12-31"
		},
		"test_date": {
			"from": "2016-01-01",
			"to": "2019-06-30"
		}
	},
	"preprocessing": {
		"sequence_length": 250,
		"sample_time_difference": 50,
