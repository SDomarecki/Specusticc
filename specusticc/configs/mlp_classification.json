{
	"import": {
		"input": [
			{
				"ticker": "PKO",
				"columns": [
					"open",
					"high",
					"low",
					"close",
					"vol"
				]
			}
		],
		"target": [
			{
				"ticker": "PKO",
				"columns": [
					"close"
				]
			}
		],
		"train_date": {
			"from": "2008-01-01",
			"to": "2015-12-31"
		},
		"test_date": {
			"from": "2016-01-01",
			"to": "2019-06-30"
		}
	},
	"preprocessing": {
		"sequence_length": 100,
		"sample_time_difference": 20,
		"sequence_prediction_time": 30
	},
	"model": {
		"target": "classification",
		"type": "neural_network",
		"layers": [
			{
				"type": "dense",
				"neurons": 500,
				"input_dim": 500,
				"activation": "sigmoid"
			},
			{
				"type": "dropout",
				"rate": 0.3
			},
			{
				"type": "dense",
				"neurons": 200
			},
			{
				"type": "dropout",
				"rate": 0.3
			},
			{
				"type": "dense",
				"neurons": 5,
				"activation": "softmax"
			}
		],
		"compilation": {
			"loss": "categorical_crossentropy",
			"optimizer": "adam",
			"metrics": [
				"categorical_accuracy"
			]
		},
		"fitting":{
			"epochs": 100,
			"batch_size": 100
		}
	}
}
